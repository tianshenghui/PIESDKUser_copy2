<?xml version="1.0" encoding="UTF-8"?>
<ImportDefinitions>


<ImportDefinition name="Geo-coded - one file per scene" description="">
<SceneSearch folders-from-file-system="yes" scene-name="{scene}">
	<TagString>{root}\{any-folders}\{{scene}.{any}:reverse}</TagString>
</SceneSearch>
<SceneDefinition geo-coding="from-file" scene-extent="union">
	<ImageLayer alias="">
		<TagString>{root}\{any-folders}\{{scene}.{any}:reverse}</TagString>
	</ImageLayer>
</SceneDefinition>
</ImportDefinition>

<ImportDefinition name="Geo-coded - one scene per folder" description="">
<SceneSearch folders-from-file-system="yes" scene-name="{scene}">
	<TagString desc="for root folder">{{root}\{scene}\{layer}.{any}:reverse}</TagString>
	<TagString desc="for sub-folders">{root}{\{any-folders}\{scene}\{layer}.{any}:reverse}</TagString>
</SceneSearch>
<SceneDefinition geo-coding="from-file" scene-extent="union">
	<ImageLayer>
		<TagString desc="for root folder">{{root}\{scene}\{layer}.{any}:reverse}</TagString>
		<TagString desc="for sub-folders">{root}{\{any-folders}\{scene}\{layer}.{any}:reverse}</TagString>
	</ImageLayer>
</SceneDefinition>
</ImportDefinition>

