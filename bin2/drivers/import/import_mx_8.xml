<?xml version="1.0" encoding="UTF-8"?>
<ImportDefinitions>
	<ImportDefinition name="MX8 - Central Camera View - AVI" description="">
		<SceneSearch folders-from-file-system="yes" bdi-driver="TRI_RUN" scene-name="{scene}">
			<TagString>{{root}\{any-folders}\{any}.RUN?run={scene}&amp;relpath={any}.avi&amp;angle=0&amp;num={num}:reverse}</TagString>
		</SceneSearch>
		<SceneDefinition force-fitting="0" geo-coding="from-file" scene-extent="union" scene-unit="auto" pixel-size="auto">
			<ImageLayer channel="1" alias="Layer0{layer-number}" driver="AVI">
				<TagString>{search-string}</TagString>
			</ImageLayer>
			<ImageLayer channel="2" alias="Layer0{layer-number}" driver="AVI">
				<TagString>{search-string}</TagString>
			</ImageLayer>
			<ImageLayer channel="3" alias="Layer0{layer-number}" driver="AVI">
				<TagString>{search-string}</TagString>
			</ImageLayer>
			<ThematicLayer alias="Trace0{layer-number}" attribute-id-column="" raster-line-width="1" raster-point-size="1" raster-diagonal-neighbourhood="no">
				<Geometry driver="SHAPE">
					<TagString>{{root}\{any-folders}\{any}.RUN?run={scene}&amp;relpath={any}_poly.shp&amp;angle=0&amp;num={any2}:reverse}</TagString>
				</Geometry>
				<Attribute driver="DBase">
					<TagString>{{root}\{any-folders}\{any}.RUN?run={scene}&amp;relpath={any}_poly.dbf&amp;angle=0&amp;num={any2}:reverse}</TagString>
				</Attribute>
			</ThematicLayer>
		</SceneDefinition>
	</ImportDefinition>

